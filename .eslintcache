leWarningCount":0},{"filePath":"108","messages":"109","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"60"},{"filePath":"110","messages":"111","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"112","messages":"113","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"114","messages":"115","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"60"},"/Users/maxcramer/Documents/Development/buger-builder/src/index.js",[],["116","117"],"/Users/maxcramer/Documents/Development/buger-builder/src/App.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/reportWebVitals.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/containers/BurgerBuilder/BurgerBuilder.js",["118"],"import React, { Component } from 'react';\nimport axios from '../../axios-orders';\n\nimport Aux from '../../hoc/Aux/Aux';\nimport Burger from '../../components/Burger/Burger';\nimport BuildControls from '../../components/Burger/BuildControls/BuildControls';\nimport Modal from '../../components/UI/Modal/Modal';\nimport OrderSummary from '../../components/Burger/OrderSummary/OrderSummary';\nimport Spinner from '../../components/UI/Spinner/Spinner';\nimport withErrorHandler from '../../hoc/withErrorHandler/withErrorHandler';\nimport { resolvePlugin } from '@babel/core';\n\n\n\nconst INGREDIENT_PRICES = {\n    salad: 0.5,\n    cheese: 0.4,\n    meat: 1.3, \n    bacon: 0.7,\n}\n\nclass BurgerBuilder extends Component{\n    // constructor(props) {\n    //     super();\n    // }\n    state = {\n        ingredients: null,\n        totalPrice: 4,\n        purchasable: false,\n        ordering: false,\n        loading: false,\n        error: false\n    }\n\n    componentDidMount () {\n        axios.get('https://react-burger-bar-ed94a-default-rtdb.firebaseio.com/ingredients.json')\n            .then(response => {\n                console.log('RESPONSE', response.data)\n                this.setState({ingredients: response.data})\n            })\n            .catch(error => {\n                this.setState({error: true})\n            })\n    }\n\n    updatePurchaseState (ingredients) {\n        const sum = Object.keys(ingredients)\n            .map(igKey => {\n                return ingredients[igKey]\n            })\n            .reduce((sum, el) => {\n                return sum + el;\n            }, 0 );\n            this.setState({purchasable: sum > 0});\n    }\n\n    addIngredientHandler = (type) => {\n        const oldCount = this.state.ingredients[type];\n        const updatedCount = oldCount + 1;\n        const updatedIngredients = {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCount;\n        const priceAddition = INGREDIENT_PRICES[type];\n        const oldPrice = this.state.totalPrice;\n        const newPrice = oldPrice + priceAddition;\n        this.setState({totalPrice: newPrice, ingredients: updatedIngredients})\n        this.updatePurchaseState(updatedIngredients);\n    }\n\n    removeIngredientHandler = (type) => {\n        const oldCount = this.state.ingredients[type];\n        if(oldCount <= 0) {\n            return;\n        }\n        const updatedCount = oldCount - 1;\n        const updatedIngredients = {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCount;\n        const priceReduction = INGREDIENT_PRICES[type];\n        const oldPrice = this.state.totalPrice;\n        const newPrice = oldPrice - priceReduction;\n        this.setState({totalPrice: newPrice, ingredients: updatedIngredients})\n        this.updatePurchaseState(updatedIngredients);\n    }\n\n    orderHandler = () => {\n        this.setState({ordering: true})\n    }\n\n    orderCancelHandler = () => {\n        this.setState({ordering: false})\n    }\n\n    orderContinueHandler = () => {\n        // alert('You continue!')\n  \n        const queryParams = [];\n        for(let i in this.state.ingredients) {\n            queryParams.push(encodeURIComponent(i) + '=' + encodeURIComponent(this.state.ingredients[i]));\n        }\n        queryParams.push('price=' + this.state.totalPrice);\n\n        const queryStirng = queryParams.join('&')\n\n        this.props.history.push({\n            pathname: '/checkout',\n            search: '?' + queryStirng\n        });\n    }\n\n    render() {\n        const disableInfo = {\n            ...this.state.ingredients\n        };\n        for( let key in disableInfo ) {\n            disableInfo[key] = disableInfo[key] <= 0;\n        }\n        let orderSummary = null;\n        if(this.state.ingredients) {\n            orderSummary =  <OrderSummary \n            ingredients={this.state.ingredients}\n            orderCancelled={this.orderCancelHandler}\n            orderContinue={this.orderContinueHandler}\n            price={this.state.totalPrice}\n        />\n        }\n        \n        if(this.state.loading) {\n            orderSummary = <Spinner />;\n        }\n        let burger = this.state.error ? <p>Ingredients can't be loaded</p> : <Spinner />;\n\n        if(this.state.ingredients) {\n            burger = (\n                <Aux>\n                    <Burger ingredients={this.state.ingredients}/>\n            <BuildControls \n                disabled={disableInfo}\n                ingredientAdded={this.addIngredientHandler}\n                ingredientRemoved={this.removeIngredientHandler}\n                price={this.state.totalPrice}\n                purchasable={this.state.purchasable}\n                ordered={this.orderHandler}\n            />\n    \n                </Aux>\n            )\n        }\n\n        return (\n            <Aux>\n                <Modal show={this.state.ordering} modalClosed={this.orderCancelHandler}>\n                   {orderSummary}\n                </Modal>\n                {burger}\n            </Aux>\n        );\n    }\n\n\n};\n\nexport default withErrorHandler(BurgerBuilder, axios);\n\n","/Users/maxcramer/Documents/Development/buger-builder/src/components/Burger/Burger.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Burger/BurgerIngredient/BurgerIngredient.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Burger/BuildControls/BuildControls.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Burger/BuildControls/BuildControl/BuildControl.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/UI/Modal/Modal.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Burger/OrderSummary/OrderSummary.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/UI/Backdrop/Backdrop.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/UI/Button/Button.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Navigation/Toolbar/Toolbar.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Logo/Logo.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Navigation/NavigationItems/NavigationItems.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Navigation/NavigationItems/NavigationItem/NavigationItem.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Navigation/SideDrawer/SideDrawer.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/Navigation/BurgerBar/BurgerBar.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/hoc/Aux/Aux.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/hoc/Layout/Layout.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/axios-orders.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/components/UI/Spinner/Spinner.js",[],"/Users/maxcramer/Documents/Development/buger-builder/src/hoc/withErrorHandler/wit